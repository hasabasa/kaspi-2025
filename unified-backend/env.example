# Database Configuration
DB_MODE=supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key

# Authentication
AUTH_METHOD=playwright

# CORS Configuration
CORS_ALLOWED_ORIGINS=^(http:\/\/localhost(:\d+)?|http:\/\/127\.0\.0\.1(:\d+)?)$

# Proxy Configuration
PROXY_ENABLED=false
PROXY_LIST=

# Demper Configuration
DEMPER_ENABLED=true
DEMPER_INTERVAL=300

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE=logs/app.log

# WAHA Configuration
WAHA_API_URL=http://localhost:3000
WAHA_ENABLED=false
WAHA_WEBHOOK_BASE_URL=http://localhost:8010

